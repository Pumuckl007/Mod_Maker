package modmaker.export;

public class File extends java.io.File{

	/**
	 * 
	 */
	private static final long serialVersionUID = -5119015006997626362L;

	public File(String pathname) {
		super(pathname.replace("/", java.io.File.pathSeparator));
	}

	@Override
	public File[] listFiles() {
		return (File[])super.listFiles();
	}
	

}
